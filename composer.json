{
  "name": "krowinski/php-inotify",
  "description": "Wraps php inotify module into object and dispatch them using event dispatcher.",
  "keywords": [
    "inotify"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Kacper Rowi≈Ñski",
      "email": "kacper.rowinski@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1",
    "ext-inotify": "*",
    "ext-json": "*",
    "symfony/event-dispatcher": "^4.0|^5.0|^6.0",
    "vistik/typed-collections": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Inotify\\": "src/Inotify/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Inotify\\Tests\\": "tests/"
    }
  }
}